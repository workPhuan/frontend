  "license": "MIT",
  "homepage": "https://github.com/vueuse/sound#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vueuse/sound.git"
  },
  "bugs": {
    "url": "https://github.com/vueuse/sound/issues"
  },
  "keywords": [
    "vue",
    "use",
    "sound",
    "composable"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
